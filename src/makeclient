mesc.out: error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o mesc.o readline.o writen.o
	cc -o mesc.out error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o mesc.o readline.o writen.o -lpthread

error.o: error.c acc.h
	cc -c error.c

wrapunix.o: wrapunix.c acc.h
	cc -c wrapunix.c

wrapsock.o: wrapsock.c acc.h
	cc -c wrapsock.c

wraplib.o: wraplib.c acc.h
	cc -c wraplib.c

wrapstdio.o: wrapstdio.c acc.h
	cc -c wrapstdio.c

mesc.o: mesc.c acc.h
	cc -c mesc.c

readline.o: readline.c acc.h
	cc -c readline.c

writen.o: writen.c acc.h
	cc -c writen.c

wrappthread.o: wrappthread.c acc.h
	cc -c wrappthread.c -lpthread

tcp_connect.o: tcp_connect.c acc.h
	cc -c tcp_connect.c -lpthread

clean:
	rm -rf *.o *~ mesc.out