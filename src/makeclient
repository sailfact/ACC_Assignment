MES-C.out: error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o MES-C.o readline.o writen.o str_mail.o
	cc -o MES-C.out str_mail.o error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o MES-C.o readline.o writen.o

error.o: error.c acc.h
	cc -c error.c

wrapunix.o: wrapunix.c acc.h
	cc -c wrapunix.c

wrapsock.o: wrapsock.c acc.h
	cc -c wrapsock.c

wraplib.o: wraplib.c acc.h
	cc -c wraplib.c

wrapstdio.o: wrapstdio.c acc.h
	cc -c wrapstdio.c

MES-C.o: MES-C.c acc.h
	cc -c MES-C.c

str_mail.o: str_mail.c acc.h
	cc -c str_mail.c

readline.o: readline.c acc.h
	cc -c readline.c

writen.o: writen.c acc.h
	cc -c writen.c
